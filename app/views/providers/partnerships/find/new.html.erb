<%= content_for(:breadcrumbs) do %>
  <%= render GovukComponent::BackLinkComponent.new(
        text: "Back",
        href: @back_path
      ) %>
<% end %>

<% page_data(title: @page_title, subtitle: @page_subtitle, caption: @page_caption, error: @form.errors.present?) %>

<%= form_for @form, url: @form_url, method: :post, html: { data: { module: "app-partners-autocomplete" } } do |form| %>
  <%= content_for(:page_alerts) { form.govuk_error_summary } %>

  <%= form.govuk_text_field :partner_id, label: nil %>

  <%#= form.govuk_text_field(
        :query,
        label: nil,
        value: query,
        "data-field" => "partners-autocomplete",
        width: "three-quarters",
      ) %>

  <%#= form.hidden_field :partner_id, id: "training-partners-id" %>
  <%= govuk_details summary_text: t(".details.summary_text"), text: t(".details.text_html") %>
  <%= form.govuk_submit "Continue" %>
<% end %>

<p class="govuk-body">
  <%= govuk_link_to("Cancel", @cancel_path) %>
</p>
